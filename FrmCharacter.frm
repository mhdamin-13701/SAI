Sub init()
    Top = 0
    Left = 0
    Dim sqlText As String
    Dim rsCharacter As New ADODB.Recordset
    sqlText = "select Code, CommidityTypeId, CommidityType, Character from  CharacterQry"
    Set rsCharacter = de.con.Execute(sqlText)
    Set FlexGrid.DataSource = rsCharacter
End Sub
Sub PrintRep()
On Error GoTo ErrorHandler
    With cr1
        .Connect = ConnectName()
        .ReportFileName = App.Path & "\reports\Character.rpt"
        .WindowState = crptMaximized
        .DiscardSavedData = True
        .Action = 1
    End With
Exit Sub
ErrorHandler:
MsgBox Err.Description
End Sub

Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
Select Case Button.Index
    Case 1:
        PrintRep
    Case 3:
        Unload Me
End Select
End Sub
